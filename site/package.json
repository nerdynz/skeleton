{
  "private": true,
  "packageManager": "pnpm@7.12.0",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "build": "rimraf ../rpc/dist/frontend/client && rimraf ../rpc/dist/frontend/server && pnpm run build:client && pnpm run build:server",
    "build:client": "vite build --ssrManifest --outDir ../rpc/dist/frontend/client",
    "build:server": "vite build -c vite.config.prod.ts --outDir ../rpc/dist/frontend/server",
    "build:serverxx": "vite build --ssr ./src/entry-server.ts --outDir ../rpc/dist/frontend/server"
  },
  "dependencies": {
    "@unhead/schema": "^1.11.7",
    "@unhead/ssr": "^1.11.7",
    "@unhead/vue": "^1.11.7",
    "@vueuse/core": "^11.1.0",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.4.0",
    "@iconify-json/carbon": "^1.2.1",
    "@rollup/plugin-url": "^8.0.2",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue-macros/volar": "^0.28.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.10.0",
    "eslint-plugin-format": "^0.1.2",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.9.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.7",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "^2.11.8",
    "unplugin-vue-router": "^0.10.7",
    "vite": "^5.4.3",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
